{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://chat-backend.azurewebsites.net/api/*"
    },
    {
      "route": "/chatHub/*",
      "rewrite": "https://chat-backend.azurewebsites.net/chatHub/*",
      "allowedMethods": ["GET", "POST"] 
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
